{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "credentials": "true",
          "headers": {
            "Access-Control-Allow-Origin": "https://gymbro-front-end.vercel.app"
          }
        }
    ]
  }
